                    @Html.Partial("~/Views/Wall/_BasicSettings.cshtml", @Model) <!--model to add as parameter-->
                </div>
                <div id="wallsettings" class="tab-pane">
                    @Html.Partial("~/Views/Wall/_AdvancedSettings.cshtml", @Model) <!--model to add as parameter-->
                </div>
        </div>
    </div>
</div>